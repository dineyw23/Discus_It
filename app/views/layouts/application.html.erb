<!DOCTYPE html>
<html>
<head>
  <title>Disc</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<% flash.each do |msg,name| %>
	<%= content_tag(:div, msg, class: "alert alert-#{name}" )%>
<%end%>
<%= yield %>

</body>
</html>
